    "import numpy as np\n",
    "\n",
    "V1= 20000\n",
    "V0= 20\n",
    "G= 20*(np.log10(V1/V0))\n",
    "print(\"20000 微巴斯卡是\",G,\"分貝\" )\n",
    "\n",
    "G1=30\n",
    "G2=50\n",
    "V1= 20*(np.power(10,G1/20))  \n",
    "V2= 20*(np.power(10,G2/20))\n",
    "\n",
    "print(\"30分貝是50分貝的\",V1/V2,\"倍\")\n",
    "\n"
   ]
